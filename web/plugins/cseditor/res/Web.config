<configuration>
  <appSettings/>
  <connectionStrings/>

  <system.web>
    <!--允许请求持续时间（秒），最大请求的长度（千字节）-->
    <!--<httpRuntime executionTimeout="30" maxRequestLength="1048576"/>-->
    <!--允许请求持续时间（秒），最大请求的长度（千字节）；这里设置了60小时，1.9G（这是最大值了，不能再大了）-->
    <httpRuntime executionTimeout="36000" maxRequestLength="2097151"/>
  </system.web>
</configuration>
